{
    "compilerOptions": {
        "module": "es6",
        "target": "es6",
        "sourceMap": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "strictNullChecks": true,
        "alwaysStrict": true,
        "strictFunctionTypes": true,
        "strict": true,
        "removeComments": false,
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "allowSyntheticDefaultImports": true
    }
}
